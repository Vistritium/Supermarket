<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.mycompany.amm.model.FinanceRegister">
        <id name="idFR">
            <column name="idFR" />
            <generator class="identity" />
        </id>
        
        <property name="price" type="java.lang.Float">
            <column name="price" length="45" not-null="true" />
        </property>
        
        <property name="data" type="java.util.Date">
            <column name="data" length="45" not-null="false" />
        </property>
        
        <property name="account" type="java.lang.Float">
            <column name="product" length="45" not-null="true" />
        </property>
        
        <property name="description" type="string">
            <column name="description" length="45" not-null="true" />
        </property>
        
        <many-to-one name="user" class="com.mycompany.amm.model.Users" fetch="select">
            <column name="iduser" not-null="true" />
        </many-to-one>
        

        
    </class>
</hibernate-mapping>
